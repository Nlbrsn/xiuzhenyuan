/******reset******/
body,div,ul,li,p,button,footer,header,nav{
margin:0;
padding:0;
}
footer,header,nav{
display:block;
}
ul,li{
list-style:none;
}
img{
border:0;
}
a{
text-decoration:none;
}

/*布局定位属性 自身属性 文本属性 其他属性 CSS3属性*/
header{

position:fixed;
top:0;
width:100%;
background-color:#5fc0cd;
text-align:center;
line-height:5rem;
vertical-align:middle;
}
header button:first-child,.btn-default:hover,
.btn-default:focus,
.btn-default:active,
.btn-default.active{
border:0;
background:#55a8b3;
color:#fff;
}
header button:last-child{
border:0;
background:#fff;
color:#5fc0cd;
}

header img{
position:absolute;
right:1rem;
top:50%;
transform:translateY(-50%);
}
.container-fluid{	
margin-top:5rem;
border-bottom:1px solid #ccc;
text-align:center;
}
div.row button:first-child{
font-size: 16px;
}
.center {
	border-right: 1px;
	border-left: 1px;
	border-top: 0;
	border-bottom: 0;
	border-style: solid;
    -webkit-border-image: 
      -webkit-linear-gradient(#fff , #ccc, #fff) 1 100%;
    -moz-border-image:
      -moz-linear-gradient(#fff , #ccc, #fff) 1 100%;    
    -o-border-image:
      -o-linear-gradient(#fff , #ccc, #fff) 1 100%;
    border-image:
      linear-gradient(to bottom, #fff , #ccc, #fff) 1 100%;
}


/*******list*******/
div.list li{
line-height: 20px;
border-bottom: 1px solid #ccc;
font-size: 16px;
}

li span{
padding-left: 1.3rem;
color: #ccc;
}
.left{
padding-left: 3.4rem;
color: black;
}
.right{
float: right;
padding-right: 2rem;
color: red;
}
a{
font-weight: bold;
color: #5fc0cd;
}
/******底部******/

footer{
position: fixed;
bottom: 0;
height: 6rem;
width: 100%;
left: 0;
right: 0;
background-color: #f8fafb; 
border-top: 1px solid #ccc;
}

.middle{
position: relative;
top: 50%;
transform: translateY(-50%);
}
p{
color: #5fc0cd;
}
.gray{
color:#ccc;
}

.round{
background-color: #5fc0cd;
width:5rem;
height:5rem;
border-radius:50%;
position:relative;
left:50%;

transform:translate(-50%);
}
footer div.row{

margin-top:.5rem;
}
/******图片******/

img{
background:url('img/img.png') no-repeat;
overflow:hidden;
}
.goal{height:23px;width:19px;background-position: 0 0;}
.date{height:24px;width:24px;background-position: 0 -50px;}
.middle{height:41px;width:40px;background-position: 0 -100px;}
.icon-biggoal{height:44px;width:42px;background-position: 0 -200px;}
.my{height:41px;width:42px;background-position: 0 -150px;}
.home{height:40px;width:46px;background-position: 0 -250px;}
/****reset****/
body,div,header{
padding:0;margin:0;
}
body{
background-color:#f0f0f0 !important;

}
/*****header*****/
header{
display:fixed;
width:100%;
top:0;
left:0;
right:0;
height:50px;
line-height:50px;
background-color:#29bde0;
}
header img{
margin-left:10px;
vertical-align:middle;
}
/****历史****/
.history,.main{
background:#fff;
width:90%;
margin:2rem auto;
border:1px solid #c9c9c9;
color:#9a9a9a;
line-height:5rem;
font-size:2rem;
}
.history{
height:5rem;
}
div.history img{
float:right;
position:relative;
top:50%;
transform:translateY(-50%);
margin-right:1rem;
}
/****主体****/
h2{
padding-bottom:20px;
margin-bottom:20px !important;
border-bottom:2px solid #fdd793;
color:#fbb435 !important;
}
.box{
background-color:#69d1e9;
color:#fff;
height:8rem;
width:80%;
line-height:8rem;
margin:0 auto;
}
p{
font-size:1.6rem;
margin:0 !important;
}
/******三角按钮*****/
.main{
position:relative;
}

.icon-btn{
position:absolute;
top:50%;
transform:translateY(-50%);
right:.7rem;
}
img{
width:15px;
}
/*****四个圆*****/
.round{
display:inline-block;
background:#f0f0f0;
border-radius:50%;
width:1.5rem;
height:1.5rem;
margin:0;

}
.first{
background:#69d1e9;
}
.cercle{
line-height:0;
padding-bottom:1rem;
}
.last{
line-height:0;
padding:2rem 0;
}
a{text-decoration:none 1important;}